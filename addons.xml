<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.nlradio" name="NL Radio" version="6.8.2" provider-name="vDia Internet Applications">
	<extension point="xbmc.python.pluginsource" library="start.py">
		<provides>audio</provides>
	</extension>
	<extension  point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="nl">Nederlandse radiostreams in een plugin verwerkt</summary>
		<description lang="nl">Luister eenvoudig naar Nederlandse radiostreams.</description>
		<disclaimer>De radiostreams worden sinds 2005 zorgvuldig verzameld en ingedeeld door bigfoot87.com</disclaimer>
		<language>nl</language>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.audio.youtube.chrisvalentine"
	name="Chris Valentine"
	version="1.0.5"
	provider-name="Chris Valentine">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.addon.common" version="2.0.0"/>
    <import addon="plugin.video.youtube" version="5.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="nl">Chris Valentine</summary>
    <description lang="nl">Dutch singer/songwriter and producer.</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
